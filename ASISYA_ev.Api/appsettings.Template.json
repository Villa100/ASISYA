{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Jwt": {
    "Issuer": "ASISYA_ev",
    "Audience": "ASISYA_ev_clients",
    "SecretKey": "<PROPORCIONAR_MEDIANTE_ENV_VAR_JWT_SECRET>",
    "ExpirationMinutes": 60
  },
  "ConnectionStrings": {
    "DefaultConnection": "Host=<DB_HOST>;Port=<DB_PORT>;Database=<DB_NAME>;Username=<DB_USER>;Password=<DB_PASSWORD>"
  },
  "CacheSettings": {
    "RedisHost": "<REDIS_HOST>:<REDIS_PORT>",
    "ListTtlMinutes": 2,
    "DetailTtlMinutes": 10
  },
  "AllowedHosts": "*"
}
